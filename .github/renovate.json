{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["github>kevinmarrec/config//packages/renovate/default"],
  "packageRules": [
    {
      "matchPackageNames": ["/@kevinmarrec/"],
      "groupName": "@kevinmarrec"
    },
    {
      "matchPackageNames": ["/eslint/"],
      "groupName": "eslint"
    },
    {
      "matchPackageNames": ["/stylelint/"],
      "groupName": "stylelint"
    },
    {
      "matchPackageNames": ["typescript"],
      "groupName": "typescript"
    },
    {
      "matchSourceUrls": ["https://github.com/unocss/unocss"],
      "groupName": "unocss"
    }
  ],
  "prHourlyLimit": 10
}
