{
  "name": "@kevinmarrec/config",
  "version": "1.10.0",
  "private": true,
  "packageManager": "bun@1.3.6",
  "author": "Kevin Marrec <kevin@marrec.io>",
  "license": "MIT",
  "homepage": "https://github.com/kevinmarrec/config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinmarrec/config.git"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "check": "bun run check:eslint && bun run check:unused && bun run check:types",
    "check:eslint": "eslint . --cache",
    "check:types": "tsc --noEmit",
    "check:unused": "knip -n",
    "lint": "bun run check:eslint",
    "lint:inspect": "bunx @eslint/config-inspector",
    "release": "bumpp",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/bun": "^1.3.7",
    "@vitest/coverage-v8": "^4.0.18",
    "bumpp": "^10.4.0",
    "eslint": "^9.39.2",
    "knip": "^5.82.1",
    "pathe": "^2.0.3",
    "tsdown": "^0.20.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
